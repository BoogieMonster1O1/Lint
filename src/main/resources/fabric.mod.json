{
  "schemaVersion": 1,
  "id": "lint",
  "version": "1.2.0",

  "name": "Lint",
  "description": "Adds lots of content to the game to extend gameplay and make it more enjoyable",
  "authors": [
    "hydos",
    "valoghese",
    "ramidzkh"
  ],

  "icon": "assets/lint/icon.png",

  "environment": "*",
  "entrypoints": {
    "mm:early_risers": [
      "io.github.hydos.lint.old.PreInitialize::initialize"
    ],
    "main": [
      "io.github.hydos.lint.Lint",
      "io.github.hydos.lint.old.Containers::onInitialize",
      "io.github.hydos.lint.old.Entities::onInitialize",
      "io.github.hydos.lint.old.Items::onInitialize"
    ],
    "client": [
      "io.github.hydos.lint.old.client.LintClient::onInitializeClient"
    ]
  },
  "mixins": [
    "mixins.lint.json"
  ],

  "depends": {
    "fabric": "*",
    "minecraft": "1.16.x"
  }
}
